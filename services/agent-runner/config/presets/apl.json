{
  "name": "APLeadFinder -- Default",
  "industry": "Generic B2B",
  "geo": {
    "radius_km": 999,
    "center_city": "",
    "state": ""
  },
  "keywords": [
    "{industry} services",
    "{city} business services",
    "professional services",
    "business consulting",
    "small business support"
  ],
  "brand_voice": "Professional, value-first, short paragraphs, plain language.",
  "from_name": "NOFA BC",
  "from_email": "info@nofabusinessconsulting.com",
