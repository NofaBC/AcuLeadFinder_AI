    "enabled": true,
    "delay_days": 5,
    "max_follow_ups": 1
  },
  "vertical_specific": {
    "healthcare": {
      "keywords_add": ["medical practice", "healthcare services", "clinic management"],
      "brand_voice_mod": "More clinical and patient-focused"
    },
    "technology": {
      "keywords_add": ["tech services", "software development", "IT consulting"],
      "brand_voice_mod": "More technical and innovation-focused"
    },
    "professional_services": {
      "keywords_add": ["consulting services", "business advisory", "professional firms"],
      "brand_voice_mod": "More formal and corporate"
    }
  }
